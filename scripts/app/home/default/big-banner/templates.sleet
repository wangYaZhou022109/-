#!handlebars
module
    .banner
        #banner(data-region='advertising')

view('advertising')
    #banner.banner.home-banner > ul
        each(adverts)
            li > a(href=linkAddress)(target='_blank')&if(outerLink) > img(src=downloadUrl(pcImage))
