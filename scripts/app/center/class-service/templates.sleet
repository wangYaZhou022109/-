#!handlebars

module > .demand-page
    .demand-title(data-region="demand-title")
    .demand-table(data-region="demand-table")
    .demand-footer(data-region="demand-footer")

view('demand-title')
    section.section.container
        .h4 班务管理
    .filter-row.container
        .filter-item
            span MIS编号
            input.input(type="text")
        .filter-item
            span 班级名称
            input.input(type="text")
        .filter-item
            span 需求方
            select.input
                option 单位名称1
                option 单位名称2
                option 单位名称3
        .filter-item
            span 实施年月
            .relative.inline-block
                input.input
                .iconfont.icon-riqi
view('demand-table')
