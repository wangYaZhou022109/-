#!handlebars

module
    div(data-region='main')

view('main')
    each(announcements)
        .clearfix.layout-mb-small
            .h3.m-bottom > echo(title)
            p > @echo(content)
            .pull-right
                .sub-text > echo('来自 ') + echo(organization.name)
                .text-tips.m-top > dateTime(lastModifyTime)
        .hr.dashed
