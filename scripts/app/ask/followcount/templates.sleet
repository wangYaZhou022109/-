#!handlebars
module 
    div(data-region='main')
view('main') 
    .section.follow
        .clearfix
            dl
                dt > echo(follow.contentCount)
                dd > string('followers')
            dl
                dt > echo(follow.topicCount)
                dd > string('answer')
            dl
                dt > echo(follow.expertCount)
                dd > string('share')
