#!handlebars
module 
    div(data-region='list')

view('list') 
    .section
        span.h3 > string('relevant') + string('expert')
        span.more(id='relevantexperts') more
        .layout-mt-small.user-picture
            .grid-row > each(expert)
                .col-md-4 > a(href="javascript:;" title=member.fullName id='expert-' + id data-name='id' data-value=id  ) > img.block(src=member.headPortrait)