#!handlebars
module
    div(data-region='list')
    
view('list')
    ul 
        li
            echo('我的提问'+'你共提问了'+questions.length+'个问题') 
        hr
    each(questions)
        ul 
            li(style='font-weight:bold')
                a(id='details-'+id data-name='id' data-value=id) > echo(title) 
                .img(style='height:30px;width:30px;float:right;') > img(src=jsonImg )
            li
                a(id='details-'+id data-name='id' data-value=id) > echo(content) 
            li
                echo(createMemberId + '提问·' + createTime ) 
            li
                echo('来自话题 ：' + topic )
                a(id='quiz-concern-' + id href='javascript:;')(class ='active') 关注  
                echo('条讨论')
                a(id='quiz-enjoy-' + id href='javascript:;')(class ='active') 分享
                echo('|')
                echo(auditStatus)
                a(id='quiz-report-' + id href='javascript:;')(class ='active') 举报
                a(id='quiz-remove-' + id href='javascript:;')(class ='active') 删除
                hr
            li 
                a('更多》》')
                    
                  

                   
                