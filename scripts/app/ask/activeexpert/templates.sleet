#!handlebars
module 
    div(data-region='list')

view('list')
    .section
        span.h3 > string('active') + string('expert')
        span.more(id='expert-more') more
        .layout-mt-small.user-picture
            .grid-row
                each(expert)
                    .col-md-4 > a
                        img.block( id='expert-' + member.id data-name='id' data-value=member.id src=member.headPortrait)
                
    