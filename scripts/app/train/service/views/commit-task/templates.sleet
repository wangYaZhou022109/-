#!handlebars
module
    .div(data-region='main')
view('main')
    each(tasks)
        div.div
            echo(name)
            dateTime(startTime+'-'+endTime)
            if(taskMember.id)
                a.btn(href='#/train/class-detail/task-detail/'+id target='_blank') > echo('查看')
            else
                a.btn(href='#/train/class-detail/task-detail/'+id target='_blank') > echo('提交')
