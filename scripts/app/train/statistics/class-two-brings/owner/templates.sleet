#!handlebars

module
    div(data-region='main')

view('main') > .input-group
    input.input(id=state.inputTextName name=state.inputTextName value=state.text 'readonly')(x-rule='required' x-marker='group')&if(state.required)
    input(type='hidden' id=state.inputName name=state.inputName value=state.id)
    .addon.btn#select > i.icon-search

view('modal') > form.vertical
    label > string('picker.owner.label.select')
    .frame
        #tree
