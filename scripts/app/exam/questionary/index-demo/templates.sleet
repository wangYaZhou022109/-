#! handlebars

module
    div(data-region='main')


view('main')
    h2 > echo(research.name)
    h2 > dateTime(research.startTime)
    h2 > dateTime(research.endTime)
    h2 > echo(research.questionaryDetail)
    h2 相关话题：
        each(research.topics)
            h3 > echo(name ' ')

    h2 相关调研
        each(relativeResearchs)
            h3 > echo(name)

    h2 他们也参加了调研
        each(relativeMembers)
            h3 > echo(name)